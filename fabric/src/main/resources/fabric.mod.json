  },
  "license": "MIT",
  "icon": "assets/teasememe/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.kulisaiji.teasememe.TeaseMemeModFabric"
    ],
    "client": [
      "com.kulisaiji.teasememe.TeaseMemeModFabricClient"
    ]
  },
  "mixins": [
    "teasememe.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.16.0",
    "minecraft": "~1.21.1",
    "java": ">=21",
    "fabric-api": "*",
    "geckolib": ">=4.5.5"
  }
}
